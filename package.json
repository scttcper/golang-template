{
  "name": "@ctrl/golang-template",
  "version": "0.0.0-placeholder",
  "description": "Basic golang template parsing in js",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "typectrl/golang-template",
  "keywords": [
    "typescript",
    "golang",
    "template",
    "templating",
    "re_replace"
  ],
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "module": "./dist/module/index.js",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "lint": "eslint -f codeframe src/**/*.ts test/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts test/**/*.ts",
    "prepare": "npm run build",
    "build": "del dist && tsc -p tsconfig.build.json && tsc -p tsconfig.module.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --no-cache",
    "semantic-release": "semantic-release"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "24.0.21",
    "@types/lodash.get": "4.4.6",
    "@types/node": "12.7.8",
    "@typescript-eslint/eslint-plugin": "2.3.2",
    "@typescript-eslint/parser": "2.3.2",
    "del-cli": "3.0.0",
    "eslint": "6.5.1",
    "eslint-config-xo-space": "0.21.0",
    "eslint-config-xo-typescript": "0.18.0",
    "eslint-plugin-import": "2.18.2",
    "jest": "24.9.0",
    "semantic-release": "15.13.24",
    "ts-jest": "24.1.0",
    "typescript": "3.6.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
