{
  "name": "@ctrl/golang-template",
  "version": "0.0.0-placeholder",
  "description": "basic golang template parsing in js",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "typectrl/golang-template",
  "keywords": [
    "typescript",
    "golang",
    "template",
    "templating",
    "re_replace"
  ],
  "main": "./src/index.js",
  "typings": "./src/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "lint": "tslint -t stylish -p .",
    "lint:fix": "tslint -t stylish -p . --fix",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "postbuild": "cp package.json dist && cp LICENSE dist && cp README.md dist && rimraf dist/test",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --no-cache",
    "semantic-release": "cd dist && semantic-release"
  },
  "dependencies": {
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@types/jest": "24.0.0",
    "@types/lodash": "^4.14.120",
    "@types/node": "10.12.24",
    "jest": "24.1.0",
    "prettier": "1.16.4",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.3",
    "ts-jest": "23.10.5",
    "ts-node": "8.0.2",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-xo": "0.14.0",
    "typescript": "3.3.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
